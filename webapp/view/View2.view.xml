<mvc:View
    controllerName="st.smarttable.controller.View2"
    xmlns:mvc="sap.ui.core.mvc"
	xmlns:core="sap.ui.core"
    displayBlock="true"
    xmlns="sap.m"
	
    xmlns:smartForm="sap.ui.comp.smartform"
    	xmlns:smartField="sap.ui.comp.smartfield">
	
		<App >
			<pages>
				<Page title="SmartForm" showNavButton="true" navButtonPress="onNavP"> 
					<content>
					<smartForm:SmartForm 
		id="smartForm"
		editTogglable="true" 
		title="Name"
		flexEnabled="false">
		<smartForm:Group label="EmployeeID">
			<smartForm:GroupElement label="EmployeeId">
				<smartField:SmartField value="{EmployeeID}" />
			</smartForm:GroupElement>
			<smartForm:GroupElement label="City">
				<smartField:SmartField value="{City}" />
			</smartForm:GroupElement>
			
			<smartForm:GroupElement label="Country">
				<smartField:SmartField value="{Country}" />
			</smartForm:GroupElement>
		</smartForm:Group>
        	<smartForm:Group label="EmployeeName">
				<smartForm:GroupElement label="FirstName">
					<smartField:SmartField value="{FirstName}" />
				</smartForm:GroupElement>
				<smartForm:GroupElement label="BirthDate">
					<smartField:SmartField value="{BirthDate}" />
				</smartForm:GroupElement>
				<smartForm:GroupElement label="Country">
					<smartField:SmartField value="{Country}" />
				</smartForm:GroupElement>
			</smartForm:Group>
		
	</smartForm:SmartForm>
						
						
		
	
					
					
					
					</content>
				</Page>
			</pages>
		</App>
	
</mvc:View>
